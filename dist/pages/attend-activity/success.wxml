<block>
    <view>
        <view style="{{anonymousState__temp}}">
            <view class="center">
                <icon type="success" size="70"></icon>
            </view><text>付款成功</text>
        </view>
        <view>
            <view class="list clear"><text class="font-default fl">活动名称</text><text class="small-font fr inaLine" style="{{anonymousState__temp2}}">{{title}}</text>
            </view>
            <view class="list clear"><text class="font-default fl">付款金额</text><text class="small-font fr">￥{{price}}</text>
            </view>
            <view class="list clear"><text class="font-default fl">参与姓名</text><text class="small-font fr">{{customerName}}</text>
            </view>
            <view class="list clear"><text class="font-default fl">参与号码</text><text class="small-font fr">{{customerPhone}}</text>
            </view>
            <view class="ml10"><text class="small-font">注：到店后出示您的手机号码即可进行礼包消费</text>
            </view>
        </view>
        <button class="littleYellow" bindtap="linkAttendList">查询参与活动</button>
    </view>
</block>